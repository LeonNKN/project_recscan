{
    "version": 2,
    "builds": [
        {
            "src": "YOLO_Trainer/detect_api.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "YOLO_Trainer/detect_api.py"
        }
    ],
    "env": {
        "PYTHONUNBUFFERED": "1"
    }
} 